{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "koushik naskar",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "hduqwhui",
  "license": "MIT",
  "build": {
    "appId": "enupal",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    },
    "win": {
      "target": "msi",
      "icon": "icons/charts.ico"
    }
  },
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4"
  }
}
