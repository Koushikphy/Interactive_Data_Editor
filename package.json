{
  "name": "interactive_data_editor",
  "version": "2.5.0",
  "main": "index.js",
  "description": "A Software to interactively edit data in a graphical manner.",
  "homepage": "https://github.com/Koushikphy/Interactive-Data-Editor",
  "author": "Koushik Naskar <koushik.naskar9@gmail.com>",
  "copyright": "Copyright Â© April,2019 Koushik Naskar",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "gulp && electron .",
    "startwatch": "gulp && electron . | gulp watch",
    "dist": "gulp && build",
    "watch": "gulp watch",
    "uplib": "gulp default"
  },
  "build": {
    "appId": "com.ide.app",
    "productName": "Interactive Data Editor",
    "asar": true,
    "dmg": {
      "contents": [{
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office",
      "desktop": {
        "Encoding": "UTF-8"
      }
    },
    "win": {
      "target": "msi"
    }
  },
  "files": [
    "**/*",
    "build/icon.*",
    "!src/*",
    "!gulpFile.js",
    "!app.iss",
    "!yarn.lock"
  ],
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "particles.js": "^2.0.0",
    "plotly.js-gl3d-dist": "^1.47.1",
    "request": "^2.88.0"
  }
}